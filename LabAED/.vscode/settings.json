{
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "c": "bash -lc 'cd $dirWithoutTrailingSlash && gcc $fileName -o $fileNameWithoutExt && ./$fileNameWithoutExt'",
    "cpp": "bash -lc 'cd $dirWithoutTrailingSlash && g++ $fileName -o $fileNameWithoutExt && ./$fileNameWithoutExt'"
  }
}
